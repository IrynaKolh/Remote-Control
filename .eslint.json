{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "plugins": [
    "prettier"
  ],  
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off"
  }
}
